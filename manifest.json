{
  "manifest_version": 2,
  "name": "Web Watcher",
  "version": "1.0.0", "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "icons": { "48": "images/webwatcher-48.png", "128": "images/webwatcher-128.png" },
  "browser_action": {
    "default_popup": "Popup.html",
    "default_title": "Web Watcher"
  }
  
}
